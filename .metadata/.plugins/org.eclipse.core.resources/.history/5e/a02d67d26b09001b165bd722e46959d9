#include "stdio.h"
#include "stm32f1xx_hal.h"

extern UART_HandleTypeDef huart1;

char my_mobile_number[]="380931482354\r\n";

void test_GSM_module(void)
{
	uint8_t size=0;
	char str[40]={0};
	uint8_t size_mas=sizeof(str);
	memset(str, 0 , sizeof(str));

	sprintf(str,"ATD+380931482354\r\n");      							// AT command
	size=sizeof(str);
	HAL_UART_Transmit(&huart1 , (uint8_t *)str, size, 0xFFFF);
}
