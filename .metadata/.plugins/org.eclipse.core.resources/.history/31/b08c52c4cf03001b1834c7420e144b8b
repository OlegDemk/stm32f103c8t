#ifndef INC_FINGERPRINT_GT_511C3_H_
#define INC_FINGERPRINT_GT_511C3_H_

#define COMMAND_START_CODE1 0x55
	#define COMMAND_START_CODE2 0xAA
	#define DEVICE_ID			0x0001

	#define OPEN					0x01


	typedef struct {
		unsigned int 	  ID;
		unsigned char 	States;
		unsigned char	  VerifyType;
	  unsigned char   reserved[2];
	} FPInfo;

	typedef struct {
		unsigned char 	Head1;
		unsigned char 	Head2;
		unsigned short	wDevId;
		unsigned int	nParam;
		unsigned short	wCmd;// or nAck
		unsigned short 	wChkSum;
	} SB_OEM_PKT;

#endif /* INC_FINGERPRINT_GT_511C3_H_ */
